{
  "name": "graphsdsl",
  "displayName": "GraphsDSL",
  "description": "Syntax highlighting for GraphsDSL language (.gph files)",
  "version": "0.3.0",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "graphsdsl",
        "aliases": [
          "GraphsDSL",
          "graphsdsl"
        ],
        "extensions": [
          ".gph"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "graphsdsl",
        "scopeName": "source.graphsdsl",
        "path": "./syntaxes/graphsdsl.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
